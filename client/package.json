  "license": "ISC",
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint",
      "jest --findRelatedTests"
    ],
    "**/*.+(css|html|md|js|jsx|json|ts|tsx)": [
      "prettier --write"
    ]
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "main": "index.ts",
  "name": "fs-web-application-client",
  "private": true,
  "scripts": {
    "build": "babel src --extensions .js,.jsx,.ts,.tsx --out-dir dist",
    "build:stage": "rm -rf dist && webpack --config ./webpack/webpack.stage.js",
    "check-types": "tsc",
    "format": "npm run prettier -- --write",
    "lint": "jest --config test-setup/jest.lint.js",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(css|html|md|ts|tsx|js|jsx|json)\"",
    "setup": "npm install && npm run validate",
    "start:mock": "webpack-dev-server --config ./webpack/webpack.mock.js",
    "start:stage": "webpack-dev-server --config ./webpack/webpack.stage.js",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:watch": "jest --watch --maxWorkers=50%",
    "validate": "npm-run-all --parallel check-types test build"
  },
  "version": "0.0.0"
}
